! font
Xft.dpi:        91.79
Xft.antialias:  true
Xft.hinting:    true
Xft.rgba:       rgb
Xft.autohint:   false
Xft.hintstyle:  hintsfull
Xft.lcdfilter:  lcddefault


! urxvt-settings

URxvt*depth:               32 
URxvt*background:          [85]#121619
URxvt*foreground:          #EEF0F2
URxvt*font:                xft:TerminessTTF Nerd Font Mono:Medium:antialias=true:size=13
URxvt*boldFont:            xft:TerminessTTF Nerd Font Mono:Bold:antialias=true:size=13
URxvt.highlightColor:      #FAAD45
URxvt.highlightTextColor:  #000000
URxvt*scrollBar:           false
URxvt*scrollBar_right:     false
URxvt*internalBorder:      25
URxvt*externalBorder:      0

! urxvt extensions

URxvt.perl-ext-common:     resize-font

